submitOrder(robotType, baseAddress, origin, destination) {
    find and assign robot()
	new CurrentOrder()
	update robotStatus()
	return message telling user that his/her order has been submitted/created
}

moveOrder(orderId, newOrderStatus){ <-- this implements both cancelOrder and confirmOrder
    update robot status
    send robot back to closest base
    update order status
    move order from currentOrder to orderHistory
}


